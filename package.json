{
  "name": "react-pwa",
  "version": "0.0.0-development",
  "homepage": "/",
  "private": true,
  "dependencies": {
    "@babel/core": "7.2.2",
    "@svgr/webpack": "4.1.0",
    "animejs": "^3.0.1",
    "axios": "^0.18.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "babel-plugin-named-asset-import": "^0.3.1",
    "babel-preset-react-app": "^7.0.2",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "chai": "^4.2.0",
    "compression-webpack-plugin": "^2.0.0",
    "cookie-parser": "^1.4.4",
    "css-loader": "1.0.0",
    "date-fns": "^1.30.1",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "2.0.0",
    "frappe-charts": "^1.2.0",
    "fs-extra": "7.0.1",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "http-proxy-middleware": "^0.19.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.2",
    "jest-resolve": "23.6.0",
    "jest-watch-typeahead": "^0.2.1",
    "localforage": "^1.7.3",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "0.5.0",
    "mocha": "^6.1.4",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.2.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.5.0",
    "postcss-safe-parser": "4.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-animate-height": "^2.0.9",
    "react-app-polyfill": "^0.2.2",
    "react-component-library": "^1.0.1",
    "react-dev-utils": "^8.0.0",
    "react-dom": "^16.8.4",
    "react-focus-lock": "^2.0.3",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.0",
    "react-spring": "^8.0.20",
    "react-use-gesture": "^5.0.2",
    "redux": "^4.0.1",
    "redux-first-router": "^2.1.1",
    "resolve": "1.10.0",
    "sass-loader": "7.1.0",
    "semantic-release": "^15.13.3",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.2.2",
    "url-loader": "1.1.2",
    "webpack": "4.28.3",
    "webpack-dev-server": "3.1.14",
    "webpack-manifest-plugin": "2.0.4",
    "winston": "^3.2.1",
    "workbox-webpack-plugin": "3.6.3"
  },
  "scripts": {
    "start": "node frontend/scripts/start.js",
    "build": "node frontend/scripts/build.js",
    "test": "node frontend/scripts/test.js",
    "serve": "node backend/HTTPServer/index.js",
    "lint": "./node_modules/.bin/eslint ./frontend --fix && ./node_modules/.bin/eslint ./backend --fix",
    "semantic-release": "npx semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EricCatlin/react-pwa.git"
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/git"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "frontend/src/**/*.{js,jsx,ts,tsx}",
      "!frontend/src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/frontend/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/frontend/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/frontend/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/frontend/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "<rootDir>/node_modules/jest-watch-typeahead/filename.js",
      "<rootDir>/node_modules/jest-watch-typeahead/testname.js"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "webpack-bundle-analyzer": "^3.3.2"
  }
}
